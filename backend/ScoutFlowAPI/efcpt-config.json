{
  "$schema": "https://raw.githubusercontent.com/ErikEJ/EFCorePowerTools/master/samples/efcpt-schema.json",
  "code-generation": {
    "enable-on-configuring": false,
    "generate-mermaid-diagram": false,
    "merge-dacpacs": false,
    "refresh-object-lists": true,
    "remove-defaultsql-from-bool-properties": false,
    "soft-delete-obsolete-files": true,
    "t4-template-path": null,
    "type": "all",
    "use-alternate-stored-procedure-resultset-discovery": false,
    "use-data-annotations": false,
    "use-database-names": false,
    "use-decimal-data-annotation-for-sproc-results": true,
    "use-inflector": true,
    "use-legacy-inflector": false,
    "use-many-to-many-entity": false,
    "use-nullable-reference-types": true,
    "use-prefix-navigation-naming": false,
    "use-t4": false
  },
  "file-layout": {
    "output-dbcontext-path": "Data",
    "output-path": "Models"
  },
  "names": {
    "dbcontext-name": "ScoutFlowContext",
    "dbcontext-namespace": null,
    "model-namespace": null,
    "root-namespace": "ScoutFlowAPI"
  },
  "tables": [
    {
      "name": "public.test"
    },
    {
      "name": "public._prisma_migrations"
    },
    {
      "name": "public.event"
    },
    {
      "name": "public.local"
    },
    {
      "name": "public.unit"
    },
    {
      "name": "public.user_metadata"
    },
    {
      "name": "public.role"
    },
    {
      "name": "public.user_metadataONevent"
    },
    {
      "name": "public.user_metadataONrole"
    },
    {
      "name": "public.user_metadataONunit"
    },
    {
      "name": "public.pending_user"
    },
    {
      "name": "public.pending_userONrole"
    },
    {
      "name": "public.pending_userONunit"
    }
  ],
  "type-mappings": {
    "use-DateOnly-TimeOnly": true,
    "use-HierarchyId": false,
    "use-NodaTime": false,
    "use-spatial": false
  }
}